{
    "Version":"2012-10-17",
    "Statement":[
       {
          "Sid":"VisualEditor1",
          "Effect":"Allow",
          "Action":[
             "s3:ListAllMyBuckets"
          ],
          "Resource":"*"
       },
       {
          "Sid":"VisualEditor2",
          "Effect":"Allow",
          "Action":[
             "s3:ListBucket"
          ],
          "Resource":[
             "arn:aws:s3:::playground*"
          ]
       },
       {
          "Sid":"VisualEditor3",
          "Effect":"Allow",
          "Action":[
             "s3:PutObject",
             "s3:GetObject"
          ],
          "Resource":[
             "arn:aws:s3:::playground*/*"
          ]
       },
       {
          "Sid":"VisualEditor4",
          "Effect":"Allow",
          "Action":[
             "s3:PutObject",
             "s3:GetObject",
             "s3:ListBucket"
          ],
          "Resource":[
             "arn:aws:s3:::playground*"
          ]
       }
    ]
 }