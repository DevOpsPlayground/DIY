{
    "Version":"2012-10-17",
    "Statement":[
       {
          "Sid":"s3AllResources",
          "Effect":"Allow",
          "Action":[
             "s3:ListAllMyBuckets"
          ],
          "Resource":"*"
       },
       {
          "Sid":"s3Playground",
          "Effect":"Allow",
          "Action":[
             "s3:ListBucket",
             "s3:PutObject",
             "s3:GetObject",
             "s3:PutObject",
             "s3:GetObject",
             "s3:ListBucket"
          ],
          "Resource":[
             "arn:aws:s3:::playground*"
          ]
       }
    ]
 }